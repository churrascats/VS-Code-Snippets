	"Java Bootstrap": {
		"prefix": "psvm",
		"body": "\tpublic static void main(String[] args){\n\t\t$0\n\t}"
	},
	"Generate a Public Class": {
		"prefix": "gpc",
		"body": "public class $0{\n\n}"
	},
	"Generate a Public Interface": {
		"prefix": "gpi",
		"body": "public interface $0{\n\n}"
	},
	"Generate a public void signature": {
		"prefix": "pv",
		"body": "public void $0"
	},
	"Generate a public String signature": {
		"prefix": "ps",
		"body": "public String $0"
	},
	"Generate a public int signature": {
		"prefix": "pi",
		"body": "public int $0"
	},
	"Generate a public double signature": {
		"prefix": "pd",
		"body": "public double $0"
	},
	"Generate a public long signature": {
		"prefix": "pl",
		"body": "public long $0"
	},
	"Generate a public list signature": {
		"prefix": "plist",
		"body": "public List<Object> $0"
	},
	"Generate a public map signature": {
		"prefix": "pmap",
		"body": "public Map<Object> $0"
	},
	"Generate a public set signature": {
